if (this.firstEnabledFeed === 'json') path = '/feed.json'\n\n      return this.$withBase(path)\n    },\n  },\n}\n</script>\n\n<style lang=\"stylus\">\n.feed\n  display flex\n  align-items center\n  color inherit\n\n  &:hover\n    color $accentColor\n</style>\n"]}]}